{
  "name": "graphql-apollo-server-express-typescript-seed",
  "version": "1.0.0",
  "description": "GraphQL Apollo-Server-Express Typescript seed",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "dev": "concurrently \"nodemon src/server.ts\" \"npm run generate\"",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "npx eslint ./src/**",
    "prepare": "npm run generate",
    "start": "npm run build && node dist/server.js",
    "start:prod": "node dist/server.js"
  },
  "keywords": [
    "apollo-server-express",
    "graphql",
    "typescript",
    "seed"
  ],
  "author": "Tal Shperling",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.5.0",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-resolvers": "2.5.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/graphql": "14.5.0",
    "@types/lodash": "4.14.178",
    "@types/mongodb": "4.0.6",
    "@types/node": "16.11.22",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "concurrently": "7.0.0",
    "eslint": "8.8.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "jsdoc": "3.6.10",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "apollo-datasource-mongodb": "^0.5.0",
    "apollo-server-express": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^16.0.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^8.0.0",
    "graphql-type-json": "^0.3.2",
    "lodash": "^4.17.20",
    "lru-cache": "^7.0.0",
    "merge-graphql-schemas": "^1.7.8",
    "moment": "^2.29.1",
    "mongodb": "^4.0.0",
    "rxjs": "^7.2.0",
    "sift": "^15.0.0",
    "winston": "^3.3.3",
    "winston3-logstash-transport": "^1.2.3"
  }
}
